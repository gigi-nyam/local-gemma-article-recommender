# LLM API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Ollama Settings (for local models)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gemma3:4b

# Gemini/OpenAI Settings
GEMINI_MODEL=gemini-3-pro-preview  # Gemini 3 Preview
GEMINI_THINKING_LEVEL=low  # low/medium/high または空欄（デフォルト）
OPENAI_MODEL=gpt-5.1  # 通常モード
# OPENAI_MODEL=gpt-5.1-chat-latest  # Instant mode (より高速)

# Default LLM Provider
# Options: ollama, gemini, openai
LLM_PROVIDER=gemini
